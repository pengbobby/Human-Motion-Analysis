function [velocity,averageSpeed]=marker_velocity(time,markerData)

% This function calculates the speed of each marker

markerCount=size(markerData,1);
duration=size(time,1);
delta_time=diff(time);
delta_markerData(duration-1,markerCount)=0;

for i=1:markerCount
    delta_markerData(:,i)=sqrt(...
        sum(...
        diff(...
        markerData(i,:,:)).^2,3));
end

calculate volocity


end